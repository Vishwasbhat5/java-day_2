package com.sonata.productJdbcApp;




public class ProdApp {
public static void main(String[] args) {
		ProductJdbc s1 = new ProductJdbc();
		s1.setProId(100);
		s1.setProName("shyam");
		s1.setProPrice(2355.55);
		productImplJdbc e1 = new productImplJdbc();
		int a = e1.save(e1);
		System.out.println(a);
			

	}



	
	}